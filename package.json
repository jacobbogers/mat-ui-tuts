			"commonjs": true,
			"es6": true,
			"mocha": true,
			"node": true,
			"shared-node-browser": true
		},
		"extends": [
			"eslint:recommended",
			"plugin:react/recommended"
		],
		"globals": {
			"console": true,
			"document": true
		},
		"parserOptions": {
			"ecmaFeatures": {
				"globalReturn": true,
				"impliedStrict": true,
				"jsx": true
			},
			"ecmaVersion": 9,
			"sourceType": "module"
		},
		"plugins": [
			"prettier",
			"import",
			"react"
		],
		"root": true,
		"rules": {
			"no-console": 0,
			"no-unused-vars": [
				2,
				{
					"varsIgnorePattern": "[iI]gnored"
				}
			],
			"react/jsx-uses-react": 2,
			"react/jsx-uses-vars": 2
		},
		"settings": {
			"react": {
				"version": "detect"
			}
		}
